<?xml version="1.0" encoding="utf-8"?>
<root>
	<default-scene>DefaultScene</default-scene>
	<initial-scene>DefaultScene</initial-scene>
	<scenes>
		<scene>
			DefaultScene
			<post-effects>
				<invoke-gui-cmd>OverrideHangarMenuButtons</invoke-gui-cmd>
				<set-visible>
					HeaderPanel
					<val>False</val>
				</set-visible>
			</post-effects>
			<runtime-effects/>
			<gui-items/>
		</scene>
	</scenes>
	<has-id>
		<checkpoint>
			StartCheckpoint
			<condition/>
			<effects/>
		</checkpoint>
	</has-id>
	<vars>
		<var-set>
			EnabledHeaderMenuButtons
			<asList/>
		</var-set>
		<var-set>
			EnabledHangarMenuButtons
			<asList/>
		</var-set>
	</vars>
	<triggers>
		<trigger>
			CheckpointController
			<type>linear-checkpoint-controller</type>
			<validate-var/>
			<sequence>
				<checkpoint>StartCheckpoint</checkpoint>
			</sequence>
			<on-effects>
				<save-checkpoint/>
			</on-effects>
		</trigger>
	</triggers>
</root>
