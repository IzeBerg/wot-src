	<rules>
		<rule>
			<id>newPlayer</id>
			<condition>
				<battlesCount>0</battlesCount>
			</condition>
		</rule>
		<rule>
			<id>player600Battles</id>
			<condition>
				<battlesCount>600</battlesCount>
			</condition>
		</rule>
		<rule>
			<id>player100Battles</id>
			<condition>
				<battlesCount>100</battlesCount>
			</condition>
		</rule>
		<rule>
			<id>player50Battles</id>
			<condition>
				<battlesCount>50</battlesCount>
			</condition>
		</rule>
		<rule>
			<id>player30Battles</id>
			<condition>
				<battlesCount>30</battlesCount>
			</condition>
		</rule>
		<rule>
			<id>player20Battles</id>
			<condition>
				<battlesCount>20</battlesCount>
			</condition>
		</rule>
		<rule>
			<id>unlockedTank5Level</id>
			<condition>
				<haveUnlockedTankLevel>5</haveUnlockedTankLevel>
			</condition>
		</rule>
		<rule>
			<id>haveTank4Level</id>
			<condition>
				<haveTankLevel>4</haveTankLevel>
			</condition>
		</rule>
		<rule>
			<id>haveTank5Level</id>
			<condition>
				<haveTankLevel>5</haveTankLevel>
			</condition>
		</rule>
		<rule>
			<id>haveTank10Level</id>
			<condition>
				<haveTankLevel>10</haveTankLevel>
			</condition>
		</rule>
		<rule>
			<id>playerWithBlueprint</id>
			<condition>
				<haveBluePrints/>
			</condition>
		</rule>
	</rules>
</root>
