<?xml version="1.0" encoding="utf-8"?>
<root>
	<htmlTemplates>
		<WTEventStartedMessage>
			<icon>InformationIcon</icon>
			<priority>medium</priority>
			<message>&lt;font color=&apos;#8C8C7E&apos;&gt;_(#white_tiger.white_tiger:notifications/eventStart/body)&lt;/font&gt;</message>
			<buttonsLayout>
				<button>
					<type>submit</type>
					<label>_(#white_tiger.white_tiger:notifications/eventStart/openBtnLabel)</label>
					<action>openWTEvent</action>
					<width>107</width>
				</button>
			</buttonsLayout>
		</WTEventStartedMessage>
		<WTEventEndedMessage>
			<icon>InformationIcon</icon>
			<priority>medium</priority>
			<message>&lt;font color=&apos;#8C8C7E&apos;&gt;_(#white_tiger.white_tiger:notifications/eventEnd/body)&lt;/font&gt;</message>
		</WTEventEndedMessage>
		<WTEventBattleVictoryResult>
			<icon>BattleResultIcon</icon>
			<bgIcon>white_tigerBgBattleResultIconVictory</bgIcon>
			<buttonsLayout>
				<button>
					<type>submit</type>
					<label>_(#white_tiger.white_tiger:notifications/battleResults/buttonLabel)</label>
					<action>showWTBattleResults</action>
				</button>
			</buttonsLayout>
			<message>&lt;font face=&quot;$TitleFont&quot; size=&quot;15&quot; color=&quot;#E9E2BF&quot;&gt;%(mainResultName)s&lt;/font&gt;&lt;br/&gt;&lt;font color=&apos;#8C8C7E&apos;&gt;_(#white_tiger.white_tiger:notifications/battleResults/event): %(eventName)s&lt;/font&gt;&lt;br/&gt;&lt;font color=&apos;#8C8C7E&apos;&gt;_(#white_tiger.white_tiger:notifications/battleResults/battle): %(arenaName)s&lt;/font&gt;&lt;br/&gt;&lt;font color=&apos;#8C8C7E&apos;&gt;_(#white_tiger.white_tiger:notifications/battleResults/team): &lt;font color=&apos;#C9C9B6&apos;&gt;%(teamName)s&lt;/font&gt;&lt;br/&gt;&lt;font color=&apos;#8C8C7E&apos;&gt;_(#white_tiger.white_tiger:notifications/battleResults/tanks): %(vehicleNames)s&lt;/font&gt;&lt;br/&gt;&lt;font color=&apos;#8C8C7E&apos;&gt;_(#messenger:serviceChannelMessages/battleResults/experience): &lt;font color=&apos;#C8C8B5&apos;&gt;%(xp)s&lt;/font&gt;%(xpEx)s&lt;/font&gt;&lt;br/&gt;&lt;font color=&apos;#8C8C7E&apos;&gt;_(#messenger:serviceChannelMessages/battleResults/credits): &lt;font color=&apos;#C5CFCF&apos;&gt;%(credits)s&lt;/font&gt;%(creditsEx)s&lt;/font&gt;%(achieves)s%(quests)s%(battlePassProgress)s%(stamps)s%(lootboxes)s%(tickets)s</message>
		</WTEventBattleVictoryResult>
		<WTEventBattleDefeatResult>
			<icon>BattleResultIcon</icon>
			<bgIcon>white_tigerBgBattleResultIconDefeat</bgIcon>
			<buttonsLayout>
				<button>
					<type>submit</type>
					<label>_(#white_tiger.white_tiger:notifications/battleResults/buttonLabel)</label>
					<action>showWTBattleResults</action>
				</button>
			</buttonsLayout>
			<message>&lt;font face=&quot;$TitleFont&quot; size=&quot;15&quot; color=&quot;#E9E2BF&quot;&gt;%(mainResultName)s&lt;/font&gt;&lt;br/&gt;&lt;font color=&apos;#8C8C7E&apos;&gt;_(#white_tiger.white_tiger:notifications/battleResults/event): %(eventName)s&lt;/font&gt;&lt;br/&gt;&lt;font color=&apos;#8C8C7E&apos;&gt;_(#white_tiger.white_tiger:notifications/battleResults/battle): %(arenaName)s&lt;/font&gt;&lt;br/&gt;&lt;font color=&apos;#8C8C7E&apos;&gt;_(#white_tiger.white_tiger:notifications/battleResults/team): &lt;font color=&apos;#C9C9B6&apos;&gt;%(teamName)s&lt;/font&gt;&lt;br/&gt;&lt;font color=&apos;#8C8C7E&apos;&gt;_(#white_tiger.white_tiger:notifications/battleResults/tanks): %(vehicleNames)s&lt;/font&gt;&lt;br/&gt;&lt;font color=&apos;#8C8C7E&apos;&gt;_(#messenger:serviceChannelMessages/battleResults/experience): &lt;font color=&apos;#C8C8B5&apos;&gt;%(xp)s&lt;/font&gt;%(xpEx)s&lt;/font&gt;&lt;br/&gt;&lt;font color=&apos;#8C8C7E&apos;&gt;_(#messenger:serviceChannelMessages/battleResults/credits): &lt;font color=&apos;#C5CFCF&apos;&gt;%(credits)s&lt;/font&gt;%(creditsEx)s&lt;/font&gt;%(achieves)s%(quests)s%(battlePassProgress)s%(stamps)s%(lootboxes)s%(tickets)s</message>
		</WTEventBattleDefeatResult>
		<WTLootBoxRewardsSysMessage>
			<type>grey</type>
			<priority>low</priority>
			<icon>InformationIcon</icon>
			<message>&lt;font face=&quot;$TitleFont&quot; size=&quot;15&quot; color=&quot;#E9E2BF&quot;&gt;_(#white_tiger.white_tiger:notifications/lootBoxes/title)&lt;/font&gt;&lt;br/&gt;%(text)s</message>
		</WTLootBoxRewardsSysMessage>
		<WTEventSwitchOnLootboxesSysMessage>
			<priority>high</priority>
			<icon>InformationIcon</icon>
			<message>&lt;font face=&quot;$TitleFont&quot; size=&quot;15&quot; color=&quot;#E9E2BF&quot;&gt;_(#white_tiger.white_tiger:notifications/lootboxes/header)&lt;/font&gt;&lt;br/&gt;&lt;font color=&apos;#8C8C7E&apos;&gt;%(text)s&lt;/font&gt;</message>
			<buttonsLayout>
				<button>
					<type>submit</type>
					<label>_(#white_tiger.white_tiger:notifications/lootboxes/switchOn/openBtnLabel)</label>
					<action>openWTEventPortals</action>
					<width>107</width>
				</button>
			</buttonsLayout>
		</WTEventSwitchOnLootboxesSysMessage>
		<WTEventTicketTokenWithdrawn>
			<icon>InformationIcon</icon>
			<priority>medium</priority>
			<message>&lt;font color=&apos;#8C8C7E&apos;&gt;%(text)s&lt;/font&gt;</message>
			<buttonsLayout>
				<button>
					<type>submit</type>
					<label>_(#event:notifications/ticketToken/withdrawn/openQuestsBtn)</label>
					<action>openWTEventQuests</action>
					<width>107</width>
				</button>
				<button>
					<type>cancel</type>
					<label>_(#event:notifications/ticketToken/withdrawn/openPurchasingBtn)</label>
					<action>openWTEventTicketPurchasing</action>
					<width>107</width>
				</button>
			</buttonsLayout>
		</WTEventTicketTokenWithdrawn>
		<WTEventProgressionSysMessage>
			<icon>InformationIcon</icon>
			<message>&lt;font face=&quot;$TitleFont&quot; size=&quot;15&quot; color=&quot;#E9E2BF&quot;&gt;_(#event:notifications/progression/header)&lt;/font&gt;&lt;br/&gt;&lt;font color=&apos;#8C8C7E&apos;&gt;%(text)s&lt;/font&gt;</message>
			<buttonsLayout>
				<button>
					<type>submit</type>
					<label>_(#event:notifications/progression/openBtnLabel)</label>
					<action>openWTEventCollection</action>
					<width>107</width>
				</button>
			</buttonsLayout>
		</WTEventProgressionSysMessage>
		<WtEventLootBoxesAutoOpenMessage>
			<priority>high</priority>
			<type>yellow</type>
			<icon>WarningIcon</icon>
			<message>&lt;font face=&quot;$TitleFont&quot; size=&quot;15&quot; color=&quot;#E9E2BF&quot;&gt;_(#messenger:serviceChannelMessages/lootBoxesAutoOpen/event/header)&lt;/font&gt;&lt;br/&gt;&lt;font face=&quot;$FieldFont&quot; size=&quot;14&quot; color=&apos;#8C8C7E&apos;&gt;_(#messenger:serviceChannelMessages/lootBoxesAutoOpen/event/text)&lt;/font&gt;&lt;br/&gt;_(#messenger:serviceChannelMessages/lootBoxesAutoOpen/event/boxes)&lt;br/&gt;%(rewards)s</message>
		</WtEventLootBoxesAutoOpenMessage>
	</htmlTemplates>
</root>
