					</milestone>
					<milestone>
						<name>playerData</name>
						<percent>86</percent>
						<text>#game_loading:player/status/download/playerData</text>
					</milestone>
					<milestone>
						<name>enter</name>
						<percent>95</percent>
						<text>#game_loading:player/status/enter</text>
					</milestone>
					<milestone>
						<name>loadHangarSpace</name>
						<percent>96</percent>
						<text>#game_loading:player/status/loadHangarSpace</text>
					</milestone>
					<milestone>
						<name>loadHangarSpaceVehicle</name>
						<percent>97</percent>
						<text>#game_loading:player/status/loadHangarSpaceVehicle</text>
					</milestone>
					<milestone>
						<name>updateVehicle</name>
						<percent>98</percent>
						<text>#game_loading:player/status/updateVehicle</text>
					</milestone>
					<milestone>
						<name>hangarUIReady</name>
						<percent>99</percent>
						<text>#game_loading:player/status/hangarUIReady</text>
					</milestone>
					<milestone>
						<name>hangarReady</name>
						<percent>100</percent>
						<forceApply>True</forceApply>
						<text>#game_loading:player/status/hangarReady</text>
					</milestone>
					<milestone>
						<name>bootcampEnqueued</name>
						<percent>95</percent>
						<text>#game_loading:player/status/bootcampEnqueued</text>
					</milestone>
					<milestone>
						<name>loadContent</name>
						<percent>100</percent>
						<forceApply>True</forceApply>
						<text>#game_loading:player/status/loadContent</text>
					</milestone>
				</bootcamp>
			</milestones>
		</player>
	</progressSettings>
</root>
