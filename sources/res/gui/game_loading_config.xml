<?xml version="1.0" encoding="utf-8"?>
<root>
	<active>true</active>
	<viewPath>coui://gui/gameface/_dist/production/loading/GameLoadingView/GameLoadingView.html</viewPath>
	<script>gui.game_loading.loading</script>
	<verbose>false</verbose>
	<fps>30</fps>
	<firstScreenTransitionDuration>
		0.4
		<!--BW_String-->
	</firstScreenTransitionDuration>
	<firstScreenDuration>5</firstScreenDuration>
	<firstScreen>
		<bg>
			<minWidth>1920</minWidth>
			<minHeight>1080</minHeight>
			<objectFit>cover</objectFit>
			<texture>gui/maps/loading/login_bg.png</texture>
			<yOffset>60</yOffset>
		</bg>
		<logo>
			<texture>gui/maps/loading/sponsors_logo.png</texture>
			<adaptive>
				<texture>gui/maps/loading/sponsors_logo_big.png</texture>
				<minWidth>1366</minWidth>
				<minHeight>768</minHeight>
			</adaptive>
		</logo>
		<ageRating>
			<texture>gui/maps/loading/age_rating_plus_transactions.png</texture>
			<hAnchor>R</hAnchor>
			<vAnchor>T</vAnchor>
			<yOffset>46</yOffset>
			<xOffset>-37</xOffset>
		</ageRating>
	</firstScreen>
	<splashScreen>
		<show>true</show>
		<module>gui.game_loading.view.splash_screen</module>
		<factory>createSplashScreen</factory>
		<canSkip>true</canSkip>
	</splashScreen>
	<states>
		<clientLoading>
			<showVfx>false</showVfx>
			<contentState>1</contentState>
		</clientLoading>
		<login>
			<showVfx>false</showVfx>
			<contentState>0</contentState>
		</login>
		<playerLoading>
			<showVfx>false</showVfx>
			<contentState>1</contentState>
		</playerLoading>
	</states>
	<logoList>
		<logo>
			<type>1</type>
			<duration>5</duration>
			<showCopyright>true</showCopyright>
			<showVersion>true</showVersion>
			<transitionDuration>
				0.4
				<!--BW_String-->
			</transitionDuration>
			<info>
				<ko>
					<text>#game_loading:gameInfo</text>
					<style>korea</style>
				</ko>
			</info>
		</logo>
	</logoList>
	<ageRatingPath>
		<default>gui/maps/loading/age_rating_plus_transactions.png</default>
		<ko>gui/maps/loading/age_rating_ko.png</ko>
	</ageRatingPath>
	<slideTransitionDuration>
		0.4
		<!--BW_String-->
	</slideTransitionDuration>
	<loginNextSlideDuration>86400</loginNextSlideDuration>
	<slideDuration>86400</slideDuration>
	<defaultSlideList>
		<slide>
			<image>gui/maps/loading/defaults/loading_01.jpg</image>
			<title>#game_loading:defaultSlide/1/title</title>
			<description>#game_loading:defaultSlide/1/description</description>
		</slide>
		<slide>
			<image>gui/maps/loading/defaults/loading_02.jpg</image>
			<title>#game_loading:defaultSlide/2/title</title>
			<description>#game_loading:defaultSlide/2/description</description>
		</slide>
		<slide>
			<image>gui/maps/loading/defaults/loading_03.jpg</image>
			<title>#game_loading:defaultSlide/3/title</title>
			<description>#game_loading:defaultSlide/3/description</description>
		</slide>
		<slide>
			<image>gui/maps/loading/defaults/loading_04.jpg</image>
			<title>#game_loading:defaultSlide/4/title</title>
			<description>#game_loading:defaultSlide/4/description</description>
		</slide>
		<slide>
			<image>gui/maps/loading/defaults/loading_05.jpg</image>
			<title>#game_loading:defaultSlide/5/title</title>
			<description>#game_loading:defaultSlide/5/description</description>
		</slide>
		<slide>
			<image>gui/maps/loading/defaults/loading_06.jpg</image>
			<title>#game_loading:defaultSlide/6/title</title>
			<description>#game_loading:defaultSlide/6/description</description>
		</slide>
		<slide>
			<image>gui/maps/loading/defaults/loading_07.jpg</image>
			<title>#game_loading:defaultSlide/7/title</title>
			<description>#game_loading:defaultSlide/7/description</description>
		</slide>
		<slide>
			<image>gui/maps/loading/defaults/loading_08.jpg</image>
			<title>#game_loading:defaultSlide/8/title</title>
			<description>#game_loading:defaultSlide/8/description</description>
		</slide>
		<slide>
			<image>gui/maps/loading/defaults/loading_09.jpg</image>
			<title>#game_loading:defaultSlide/9/title</title>
			<description>#game_loading:defaultSlide/9/description</description>
		</slide>
		<slide>
			<image>gui/maps/loading/defaults/loading_10.jpg</image>
			<title>#game_loading:defaultSlide/10/title</title>
			<description>#game_loading:defaultSlide/10/description</description>
		</slide>
	</defaultSlideList>
	<statusTextDuration>13</statusTextDuration>
	<statusTextList>
		<text>#game_loading:client/status/load</text>
	</statusTextList>
	<progressSettings>
		<client>
			<settings>
				<startPercent>0</startPercent>
				<limitPercent>50</limitPercent>
				<ticksInProgress>2000</ticksInProgress>
				<minTickTimeSec>0</minTickTimeSec>
			</settings>
			<milestones/>
		</client>
		<player>
			<settings>
				<startPercent>50</startPercent>
				<limitPercent>100</limitPercent>
				<ticksInProgress>1000</ticksInProgress>
				<minTickTimeSec>
					0.016
					<!--BW_String-->
				</minTickTimeSec>
			</settings>
			<milestones>
				<connection>
					<milestone>
						<name>connection</name>
						<percent>100</percent>
						<text>#game_loading:player/status/login</text>
					</milestone>
				</connection>
				<standard>
					<milestone>
						<name>enter</name>
						<percent>55</percent>
						<text>#game_loading:player/status/enter</text>
					</milestone>
					<milestone>
						<name>shop</name>
						<percent>73</percent>
						<text>#game_loading:player/status/download/shop</text>
					</milestone>
					<milestone>
