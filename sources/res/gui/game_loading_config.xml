						<text>#game_loading:player/status/download/discounts</text>
					</milestone>
					<milestone>
						<name>recycleBin</name>
						<percent>78</percent>
						<text>#game_loading:player/status/download/recycleBin</text>
					</milestone>
					<milestone>
						<name>playerData</name>
						<percent>79</percent>
						<text>#game_loading:player/status/download/playerData</text>
					</milestone>
					<milestone>
						<name>loadHangarSpace</name>
						<percent>89</percent>
						<text>#game_loading:player/status/loadHangarSpace</text>
					</milestone>
					<milestone>
						<name>loadHangarSpaceVehicle</name>
						<percent>90</percent>
						<text>#game_loading:player/status/loadHangarSpaceVehicle</text>
					</milestone>
					<milestone>
						<name>updateVehicle</name>
						<percent>93</percent>
						<text>#game_loading:player/status/updateVehicle</text>
					</milestone>
					<milestone>
						<name>hangarUIReady</name>
						<percent>99</percent>
						<text>#game_loading:player/status/hangarUIReady</text>
					</milestone>
					<milestone>
						<name>hangarReady</name>
						<percent>100</percent>
						<forceApply>True</forceApply>
						<text>#game_loading:player/status/hangarReady</text>
					</milestone>
					<milestone>
						<name>onboardingEnqueued</name>
						<percent>95</percent>
						<text>#game_loading:player/status/onboardingEnqueued</text>
					</milestone>
					<milestone>
						<name>loadContent</name>
						<percent>100</percent>
						<forceApply>True</forceApply>
						<text>#game_loading:player/status/loadContent</text>
					</milestone>
				</standard>
				<bootcamp>
					<milestone>
						<name>synchronize</name>
						<percent>60</percent>
						<text>#game_loading:player/status/synchronize</text>
					</milestone>
					<milestone>
						<name>shop</name>
						<percent>70</percent>
						<text>#game_loading:player/status/download/shop</text>
					</milestone>
					<milestone>
						<name>inventory</name>
						<percent>71</percent>
						<text>#game_loading:player/status/download/inventory</text>
					</milestone>
					<milestone>
