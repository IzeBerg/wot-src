<?xml version="1.0" encoding="utf-8"?>
<root>
	<gameface>
		<deffontfamily>WarheliosZHCN</deffontfamily>
		<additional>Pacifico</additional>
		<additional>DINRoundPro</additional>
		<hieroglyphical>true</hieroglyphical>
		<fontfamilyfallback>Warhelios</fontfamilyfallback>
	</gameface>
	<config>
		<name>All</name>
		<fontlib>fonts_zh_cn_sg.swf</fontlib>
		<map>
			<alias>
				<embedded>$TitleFont</embedded>
				<runtime>TahomaZH</runtime>
			</alias>
			<alias>
				<embedded>$FieldFont</embedded>
				<runtime>TahomaZH</runtime>
			</alias>
			<alias>
				<embedded>$TextFont</embedded>
				<runtime>TahomaZH</runtime>
			</alias>
			<alias>
				<embedded>$UniversCondC</embedded>
				<runtime>TahomaZH</runtime>
				<scaleFactor>
					0.98
					<!--BW_String-->
				</scaleFactor>
			</alias>
		</map>
	</config>
</root>
