<?xml version="1.0" encoding="utf-8"?>
<root>
	<htmlTemplatesSimple>
		<hbShopBundleSimpleText>&lt;font color=&apos;#8C8C7E&apos;&gt;%(text)s&lt;/font&gt;</hbShopBundleSimpleText>
	</htmlTemplatesSimple>
	<htmlTemplates>
		<HBStartedMessage>
			<priority>medium</priority>
			<type>grey</type>
			<icon>InformationIcon</icon>
			<filters>
				<filter>
					<name>glow</name>
					<color>EA5200</color>
				</filter>
				<filter>
					<name>dropshadow</name>
					<color>
						000000
						<!--BW_String-->
					</color>
				</filter>
			</filters>
			<buttonsLayout>
				<button>
					<type>submit</type>
					<label>_(#historical_battles.hb_lobby:serviceChannelMessages/HBStartedMessage/button)</label>
					<action>OnHBStartedMessage</action>
				</button>
			</buttonsLayout>
			<message>&lt;font color=&apos;#8C8C7E&apos;&gt;_(#historical_battles.hb_lobby:system_messages/switched_on_event/body)&lt;/font&gt;</message>
		</HBStartedMessage>
		<FinancialOperationCreditsSysMessage>
			<priority>low</priority>
			<type>yellow</type>
			<icon>CreditsIcon</icon>
			<message>&lt;font face=&quot;$TitleFont&quot; size=&quot;15&quot; color=&quot;#E9E2BF&quot;&gt;_(#messenger:serviceChannelMessages/currencyUpdate/financial_transaction)&lt;/font&gt;&lt;br/&gt;&lt;font color=&apos;#8C8C7E&apos;&gt;%(text)s&lt;/font&gt;&lt;/font&gt;</message>
		</FinancialOperationCreditsSysMessage>
		<HBShopBundlePurchasedSysMessage>
			<priority>low</priority>
			<type>yellow</type>
			<icon>GoldIcon</icon>
			<message>&lt;font face=&quot;$TitleFont&quot; size=&quot;15&quot; color=&quot;#E9E2BF&quot;&gt;_(#messenger:serviceChannelMessages/sysMsg/titles/financialTransaction)&lt;/font&gt;&lt;br/&gt;%(op)s</message>
		</HBShopBundlePurchasedSysMessage>
		<HBCouponsBundlePurchasedSysMessage>
			<priority>medium</priority>
			<type>yellow</type>
			<icon>GoldIcon</icon>
			<message>&lt;font face=&quot;$TitleFont&quot; size=&quot;15&quot; color=&quot;#E9E2BF&quot;&gt;_(#messenger:serviceChannelMessages/sysMsg/titles/financialTransaction)&lt;/font&gt;&lt;br/&gt;%(op)s</message>
		</HBCouponsBundlePurchasedSysMessage>
		<HBMainPrizeVehicleBundlePurchased>
			<priority>medium</priority>
			<type>yellow</type>
			<icon>PremiumTankIcon</icon>
			<message>&lt;font face=&quot;$TitleFont&quot; size=&quot;15&quot; color=&quot;#E9E2BF&quot;&gt;_(#messenger:serviceChannelMessages/invoiceReceived/invoice)&lt;/font&gt;&lt;br/&gt;%(op)s</message>
		</HBMainPrizeVehicleBundlePurchased>
		<HBBattleVictoryResult>
			<type>battleResult</type>
			<icon>BattleResultIcon</icon>
			<bgIcon>BgBattleResultSE22EventVictory</bgIcon>
			<buttonsLayout>
				<button>
					<type>submit</type>
					<label>_(#messenger:serviceChannelMessages/battleResults/button)</label>
					<action>showBattleResults</action>
				</button>
			</buttonsLayout>
			<message>&lt;font face=&quot;$TitleFont&quot; size=&quot;15&quot; color=&quot;#E9E2BF&quot;&gt;_(#messenger:serviceChannelMessages/battleResults/victory)!&lt;/font&gt;
&lt;font color=&apos;#8C8C7E&apos;&gt;%(hbFrontName)s&lt;/font&gt;
&lt;font color=&apos;#8C8C7E&apos;&gt;_(#messenger:serviceChannelMessages/battleResults/battle): %(arenaName)s&lt;/font&gt;
&lt;font color=&apos;#8C8C7E&apos;&gt;_(#historical_battles.messenger.extend:serviceChannelMessages/battleResults/hb_tanks): %(vehicleNames)s&lt;/font&gt;
&lt;font color=&apos;#8C8C7E&apos;&gt;%(hbFrontman)s&lt;/font&gt;&lt;br/&gt;%(hbExtraData)s</message>
		</HBBattleVictoryResult>
		<HBBattleDefeatResult>
			<type>battleResult</type>
			<icon>BattleResultIcon</icon>
			<bgIcon>BgBattleResultSE22EventDefeat</bgIcon>
			<buttonsLayout>
				<button>
					<type>submit</type>
					<label>_(#messenger:serviceChannelMessages/battleResults/button)</label>
					<action>showBattleResults</action>
				</button>
			</buttonsLayout>
			<message>&lt;font face=&quot;$TitleFont&quot; size=&quot;15&quot; color=&quot;#E9E2BF&quot;&gt;_(#messenger:serviceChannelMessages/battleResults/ended)!&lt;/font&gt;
&lt;font color=&apos;#8C8C7E&apos;&gt;%(hbFrontName)s&lt;/font&gt;
&lt;font color=&apos;#8C8C7E&apos;&gt;_(#messenger:serviceChannelMessages/battleResults/battle): %(arenaName)s&lt;/font&gt;
&lt;font color=&apos;#8C8C7E&apos;&gt;_(#historical_battles.messenger.extend:serviceChannelMessages/battleResults/hb_tanks): %(vehicleNames)s&lt;/font&gt;
&lt;font color=&apos;#8C8C7E&apos;&gt;%(hbFrontman)s&lt;/font&gt;&lt;br/&gt;%(hbExtraData)s</message>
		</HBBattleDefeatResult>
		<HistoricalBattlesArenaBanSystemMessage>
			<type>grey</type>
			<priority>medium</priority>
			<message>&lt;font face=&quot;$TitleFont&quot; size=&quot;15&quot; color=&quot;#E9E2BF&quot;&gt;%(header)s&lt;/font&gt;&lt;br/&gt;&lt;font size=&quot;14&quot; color=&apos;#8C8C7E&apos;&gt;%(body)s&lt;/font&gt;&lt;br/&gt;</message>
			<buttonsLayout>
				<button>
					<type>submit</type>
					<label>_(#historical_battles.hb_lobby:sysMessageFairPlayMsg/arenaBan/button)</label>
					<action>showHistoricalBattlesFairPlayWindow</action>
				</button>
			</buttonsLayout>
		</HistoricalBattlesArenaBanSystemMessage>
		<HistoricalBattlesArenaWarningSystemMessage>
			<type>grey</type>
			<icon>hbWarningIcon</icon>
			<priority>medium</priority>
			<message>&lt;font face=&quot;$TitleFont&quot; size=&quot;15&quot; color=&quot;#E9E2BF&quot;&gt;%(header)s&lt;/font&gt;&lt;br/&gt;&lt;font size=&quot;14&quot; color=&apos;#8C8C7E&apos;&gt;%(body)s&lt;/font&gt;&lt;br/&gt;</message>
			<buttonsLayout>
				<button>
					<type>submit</type>
					<label>_(#historical_battles.hb_lobby:sysMessageFairPlayMsg/arenaWarning/button)</label>
					<action>showHistoricalBattlesWarningFairPlayWindow</action>
				</button>
			</buttonsLayout>
		</HistoricalBattlesArenaWarningSystemMessage>
		<HistoricalBattlesProgressionSystemMessage>
			<type>grey</type>
			<icon>InformationIcon</icon>
			<priority>low</priority>
			<message>&lt;font face=&quot;$TitleFont&quot; size=&quot;15&quot; color=&quot;#E9E2BF&quot;&gt;%(header)s&lt;/font&gt;&lt;br/&gt;&lt;font color=&apos;#8C8C7E&apos;&gt;%(body)s&lt;/font&gt;&lt;br/&gt;&lt;font color=&apos;#8C8C7E&apos;&gt;_(#historical_battles.historical_battles_progression:serviceChannelMessages/awards)&lt;/font&gt;&lt;br/&gt;%(awards)s</message>
			<buttonsLayout>
				<button>
					<type>submit</type>
					<label>_(#historical_battles.historical_battles_progression:serviceChannelMessages/progressionButton)</label>
					<action>showHistoricalBattlesProgression</action>
				</button>
			</buttonsLayout>
		</HistoricalBattlesProgressionSystemMessage>
	</htmlTemplates>
</root>
