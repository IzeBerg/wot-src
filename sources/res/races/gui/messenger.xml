<?xml version="1.0" encoding="utf-8"?>
<root>
	<htmlTemplates>
		<racesBattleResult>
			<type>battleResult</type>
			<icon>BattleResultIcon</icon>
			<bgIcon>BgRacesResultIcon</bgIcon>
			<buttonsLayout>
				<button>
					<type>submit</type>
					<label>_(#messenger:serviceChannelMessages/battleResults/button)</label>
					<action>showBattleResults</action>
				</button>
			</buttonsLayout>
			<message>&lt;font face=&quot;$TitleFont&quot; size=&quot;15&quot; color=&quot;#E9E2BF&quot;&gt;%(racesTitle)s&lt;/font&gt;
&lt;font color=&apos;#929290&apos;&gt;_(#races.races_messenger:serviceChannelMessages/racesBattleResults/mode): &quot;%(gameMode)s&quot;&lt;/font&gt;&lt;br&gt;&lt;font color=&apos;#929290&apos;&gt;_(#races.races_messenger:serviceChannelMessages/racesBattleResults/vehicle): %(vehicleNames)s&lt;/font&gt;&lt;br&gt;&lt;font color=&apos;#929290&apos;&gt;_(#races.races_messenger:serviceChannelMessages/racesBattleResults/finishedPlace): &lt;font color=&apos;#E9E2BF&apos;&gt;%(finishedPlace)s&lt;/font&gt;&lt;/font&gt;&lt;br&gt;&lt;font color=&apos;#929290&apos;&gt;_(#races.races_messenger:serviceChannelMessages/racesBattleResults/totalScore): &lt;font color=&apos;#E9E2BF&apos;&gt;%(racesTotalScore)s&lt;/font&gt;&lt;/font&gt;&lt;br&gt;&lt;font color=&apos;#929290&apos;&gt;%(questsMsg)s&lt;/font&gt;&lt;font color=&apos;#929290&apos;&gt;%(racesFirstWinAchievement)s&lt;/font&gt;</message>
		</racesBattleResult>
