msgid "alice/LastName"
msgstr "塞列兹涅娃"

msgid "alice/Name"
msgstr "艾丽莎"

msgid "cosm_adams/Descr"
msgstr "奇迹号飞船团队的一名指挥官，该飞船已成功抵达火星。"

msgid "cosm_adams/LastName"
msgstr "亚当斯"

msgid "cosm_adams/Name"
msgstr "欧若拉"

msgid "cosm_farmer/Descr"
msgstr "奇迹号飞船团队的一名宇航员，该飞船已成功抵达火星。"

