msgid "confirmSkills/buttonApprove"
msgstr "Обменять"

msgid "confirmSkills/buttonDecline"
msgstr "Отменить"

msgid "confirmSkills/desc"
msgstr ""
"%(name)s изучит «{whiteSpanish_Open}%(skill)s{whiteSpanish_Close}» до уровня"
" {whiteSpanish_Open}%(level)s%{whiteSpanish_Close} за"

msgid "confirmSkills/head"
msgstr "Обменять свободный опыт?"

msgid "confirmSkills/percent"
