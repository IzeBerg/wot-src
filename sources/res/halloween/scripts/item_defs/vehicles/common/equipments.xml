			<shotsNumber>1</shotsNumber>
			<explodeDestructible>false</explodeDestructible>
			<areaRadius>15</areaRadius>
			<areaVisual/>
			<areaShow>always</areaShow>
			<noOwner>false</noOwner>
			<attackerType>DEATH_ZONE</attackerType>
			<shotSoundPreDelay>1</shotSoundPreDelay>
			<wwsoundShot>eb_ability_artillery_firing</wwsoundShot>
			<effects>
				<start>
					<groundRaycast>True</groundRaycast>
					<sequences>
						<sequence>
							<name>particles/eff_sequence/halloween/enemies/death_zone.seq</name>
						</sequence>
					</sequences>
				</start>
				<action>
					<shotEffects>epicArtilleryStrike_hw</shotEffects>
					<offsetDeviation>0</offsetDeviation>
					<repeatCount>1</repeatCount>
					<repeatDelay>
						0.3
						<!--BW_String-->
					</repeatDelay>
				</action>
			</effects>
			<actions>
				<action>
					<type>DamageVehicle</type>
					<applyTo>Team1</applyTo>
					<args>
						<damage>600</damage>
					</args>
				</action>
			</actions>
		</script>
	</artillery_deathzone_aoe_hw>
