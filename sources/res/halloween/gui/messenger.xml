			<icon>InformationIcon</icon>
			<filters>
				<filter>
					<name>glow</name>
					<color>EA5200</color>
				</filter>
				<filter>
					<name>dropshadow</name>
					<color>
						000000
						<!--BW_String-->
					</color>
				</filter>
			</filters>
			<buttonsLayout>
				<button>
					<type>submit</type>
					<label>_(#halloween.hw_messenger:serviceChannelMessages/eventLifetime/postPhase/button)</label>
