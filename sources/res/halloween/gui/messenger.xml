			<type>battleResult</type>
			<icon>InformationIcon</icon>
			<message>&lt;font face=&quot;$TitleFont&quot; size=&quot;15&quot; color=&quot;#E9E2BF&quot;&gt;_(#halloween.hw_messenger:serviceChannelMessages/dailyQuests/header)&lt;/font&gt;
